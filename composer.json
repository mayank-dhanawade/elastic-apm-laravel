{
   "name":"mayank-dhanawade/elastic-apm-laravel",
   "keywords":[  
      "laravel",
      "elastic",
      "APM",
      "Lumen",
      "monitoring",
      "debug"
   ],
   "description":"A package to integrate Elastic APM into Laravel",
   "license":"MIT",
   "require":{  
      "php":">= 7.0",
      "illuminate/database":"5.5.x|5.6.x|5.7.x|5.8.x",
      "illuminate/http":"5.5.x|5.6.x|5.7.x|5.8.x",
      "illuminate/routing":"5.5.x|5.6.x|5.7.x|5.8.x",
      "illuminate/support":"5.5.x|5.6.x|5.7.x|5.8.x",
      "ivoglent/elastic-apm-php-agent":"dev-master",
      "ramsey/uuid":"^3.0"
   },
   "require-dev":{  
      "phpunit/phpunit":"6.*"
   },
   "autoload":{  
      "psr-4":{  
         "PhilKra\\ElasticApmLaravel\\":"src/"
      }
   },
   "autoload-dev":{  
      "psr-4":{  
         "PhilKra\\Tests\\":"tests/"
      }
   },
   "extra":{  
      "laravel":{  
         "providers":[  
            "PhilKra\\ElasticApmLaravel\\Providers\\ElasticApmServiceProvider"
         ],
         "aliases":{  
            "ElasticApm":"PhilKra\\ElasticApmLaravel\\Facades\\ElasticApm"
         }
      }
   },
   "config":{  
      "optimize-autoloader":true
   },
   "authors":[  
      {  
         "name":"Mayank Dhanawade",
         "email":"mayank.dhanawade@gmail.com",
         "homepage":"https://github.com/mayank-dhanawade/elastic-apm-laravel"
      }
   ]
}
